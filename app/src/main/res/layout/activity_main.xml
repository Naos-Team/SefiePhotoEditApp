<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="#F6F6F6"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/img11"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@mipmap/main_banner10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintWidth_percent="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Naos Photo"
        android:textColor="@color/colorAccent"
        android:textSize="30sp"
        android:textStyle="bold"
        android:fontFamily="@font/poppinsregular"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05" />


    <ImageView
        android:id="@+id/img_bg1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/main_banner"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.7"
        app:layout_constraintWidth_percent="0.43" />

        <ImageView
            android:id="@+id/btn_editor"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:tint="@color/white"
            android:src="@mipmap/btn_edit2"
            app:layout_constraintBottom_toBottomOf="@+id/img_bg1"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@+id/img_bg1"
            app:layout_constraintHeight_percent="0.06"
            app:layout_constraintStart_toStartOf="@+id/img_bg1"
            app:layout_constraintTop_toTopOf="@+id/img_bg1"
            app:layout_constraintVertical_bias="0.4" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="Edit"
            android:textSize="20sp"
            android:gravity="center"
            android:textColor="@color/white"
            android:textStyle="bold"
            app:layout_constraintHeight_percent="0.05"
            app:layout_constraintEnd_toEndOf="@+id/btn_editor"
            app:layout_constraintStart_toStartOf="@+id/btn_editor"
            app:layout_constraintTop_toBottomOf="@+id/btn_editor" />

    <ImageView
        android:id="@+id/img_bg2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/main_banner"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintHorizontal_bias="0.93"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.7"
        app:layout_constraintWidth_percent="0.43" />

    <ImageView
        android:id="@+id/btn_camera"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@mipmap/btn_camera3"
        app:tint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/img_bg2"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/img_bg2"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintStart_toStartOf="@+id/img_bg2"
        app:layout_constraintTop_toTopOf="@+id/img_bg2"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="Camera"
        android:textSize="20sp"
        android:gravity="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintHeight_percent="0.05"
        app:layout_constraintEnd_toEndOf="@+id/btn_camera"
        app:layout_constraintStart_toStartOf="@+id/btn_camera"
        app:layout_constraintTop_toBottomOf="@+id/btn_camera" />

    <ImageView
        android:id="@+id/img_bg3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/main_banner"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.95"
        app:layout_constraintWidth_percent="0.43" />


    <ImageView
        android:id="@+id/btn_collage"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@mipmap/btn_collage2"
        app:tint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/img_bg3"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/img_bg3"
        app:layout_constraintHeight_percent="0.05"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/img_bg3"
        app:layout_constraintTop_toTopOf="@+id/img_bg3"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="Collage"
        android:textSize="20sp"
        android:gravity="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintHeight_percent="0.05"
        app:layout_constraintEnd_toEndOf="@+id/btn_collage"
        app:layout_constraintStart_toStartOf="@+id/btn_collage"
        app:layout_constraintTop_toBottomOf="@+id/btn_collage" />


    <ImageView
        android:id="@+id/img_bg4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/main_banner"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintHorizontal_bias="0.93"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.95"
        app:layout_constraintWidth_percent="0.43" />

    <ImageView
        android:id="@+id/btn_mycreation"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@mipmap/btn_history2"
        app:tint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/img_bg4"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/img_bg4"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/img_bg4"
        app:layout_constraintTop_toTopOf="@+id/img_bg4"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="Creation"
        android:textSize="20sp"
        android:gravity="center"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintHeight_percent="0.05"
        app:layout_constraintEnd_toEndOf="@+id/btn_mycreation"
        app:layout_constraintStart_toStartOf="@+id/btn_mycreation"
        app:layout_constraintTop_toBottomOf="@+id/btn_mycreation" />

    <ImageView
        android:id="@+id/img_shareapp_bg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/bg_ic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.04"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.11" />


    <ImageView
        android:id="@+id/img_shareapp"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/img_shareapp"
        android:tint="@color/white"
        app:layout_constraintBottom_toBottomOf="@id/img_shareapp_bg"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@id/img_shareapp_bg"
        app:layout_constraintHeight_percent="0.025"
        app:layout_constraintStart_toStartOf="@id/img_shareapp_bg"
        app:layout_constraintTop_toTopOf="@id/img_shareapp_bg" />

    <ImageView
        android:id="@+id/img_rateus_bg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/bg_ic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.04"
        app:layout_constraintHorizontal_bias="0.22"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.11" />

    <ImageView
        android:id="@+id/img_rateus"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/img_rateus"
        app:tint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/img_rateus_bg"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/img_rateus_bg"
        app:layout_constraintHeight_percent="0.025"
        app:layout_constraintStart_toStartOf="@+id/img_rateus_bg"
        app:layout_constraintTop_toTopOf="@+id/img_rateus_bg"
        />

    <ImageView
        android:id="@+id/img_privacy_bg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/bg_ic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.04"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.11" />

    <ImageView
        android:id="@+id/img_privacy"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@mipmap/btn_privacy"
        app:tint="@color/white"
        app:layout_constraintBottom_toBottomOf="@id/img_privacy_bg"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@id/img_privacy_bg"
        app:layout_constraintHeight_percent="0.025"
        app:layout_constraintStart_toStartOf="@id/img_privacy_bg"
        app:layout_constraintTop_toTopOf="@id/img_privacy_bg" />


